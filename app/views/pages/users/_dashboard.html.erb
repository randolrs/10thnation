<div class="profile-header dashboard" id="dashboard-header-for-mobile">

	<div class="centered-content">

		<ul>
			<li class="profile-header-option active" id="profile_option_posts" data-option-type="posts">
				<a>Top</a>
			</li>

			<li class="profile-header-option" id="profile_option_new" data-option-type="new">
				<a>New</a>
			</li>

		</ul>

	</div>

</div>


<div class="content">

	<div class="content-box">

		<div class="main-content-column">

			<div class="column-content" id="column_content_posts">

				<% @posts.each do |post| %>

					<%= render :partial => 'posts/list', :locals => {:post => post} %>

				<%end%>

				<div class="center">
   					
   					<%= will_paginate @posts, renderer: BootstrapPagination::Rails %>

				</div>

			</div>

			<div class="column-content" id="column_content_new" style="display:none;">
				 
				 <% current_user.new_posts.each do |post| %>

					<%= render :partial => 'posts/list', :locals => {:post => post} %>

				<%end%>

			</div>

		</div>

<!-- 		<div class="side-content-column">

			<h4>10th Nation</h4>

			<div class="content-frame stacked">

				<div class="content-panel">

					<p>Improve Your Account</p>

				</div>

			</div>

		</div> -->

	</div>

</div>